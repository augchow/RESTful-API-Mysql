{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "extends": "airbnb-base",
    "rules": {
        "arrow-body-style": [
            "error",
            "always"
        ],
        "comma-dangle": [
            "error",
            "never"
        ],
        "no-console": 0,
        "linebreak-style": 0,
"global-require": 0,
"eslint linebreak-style": [0, "error", "windows"]
    }
}